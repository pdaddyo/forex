  "scripts": {
	"clean": "rimraf dist",
	"compile": "better-npm-run compile",
	"lint": "eslint .",
	"lint:fix": "npm run lint -- --fix",
	"start": "better-npm-run start",
	"dev": "better-npm-run dev",
	"dev:nw": "npm run dev -- --nw",
	"dev:no-debug": "npm run dev -- --no_debug",
	"test": "better-npm-run test",
	"test:dev": "npm run test -- --watch",
	"deploy": "better-npm-run deploy",
	"copy": "rimraf ../d1091-academy-published-files/client && rsync -avhW --progress ./dist/ ../d1091-academy-published-files/",
	"flow:check": "babel-node client-bin/flow-check",
	"codecov": "cat coverage/*/lcov.info | codecov"
  },
  "betterScripts": {
	"compile": {
	  "command": "node ./node_modules/babel-cli/bin/babel-node.js client-bin/compile",
	  "env": {
		"DEBUG": "app:*"
	  }
	},
	"dev": {
	  "command": "nodemon --exec node ./node_modules/babel-cli/bin/babel-node.js client-bin/server",
	  "env": {
		"NODE_ENV": "development",
		"DEBUG": "app:*"
	  }
	},
	"deploy": {
	  "command": "./deploy.sh",
	  "env": {
		"NODE_ENV": "production",
		"DEBUG": "app:*"
	  }
	},
	"start": {
	  "command": "nodemon --exec node ./node_modules/babel-cli/bin/babel-node.js client-bin/server",
	  "env": {
		"DEBUG": "app:*"
	  }
	},
	"test": {
	  "command": "nodemon --exec node ./node_modules/babel-cli/bin/babel-node.js ./node_modules/karma/bin/karma start build/karma.conf",
	  "env": {
		"NODE_ENV": "test",
		"DEBUG": "app:*"
	  }
	}
  },
